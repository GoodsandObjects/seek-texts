  "verses": [
    {
      "number": 1,
      "text": "Hast thou observed him who belieth religion?"
    },
    {
      "number": 2,
      "text": "That is he who repelleth the orphan,"
    },
    {
      "number": 3,
      "text": "And urgeth not the feeding of the needy."
    },
    {
      "number": 4,
      "text": "Ah, woe unto worshippers"
    },
    {
      "number": 5,
      "text": "Who are heedless of their prayer;"
    },
    {
      "number": 6,
      "text": "Who would be seen (at worship)"
    },
    {
      "number": 7,
      "text": "Yet refuse small kindnesses!"
    }
  ]
